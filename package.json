{
  "name": "webcrypto-liner",
  "version": "0.1.37",
  "description": "A WebCrypto polyfill that \"smooths out\" the rough-edges in existing User Agent implementations.",
  "main": "build/index.js",
  "scripts": {
    "build": "npm run build:prod",
    "build:es5": "tsc",
    "build:es2015": "tsc -p tsconfig.es2015.json",
    "build:webpack": "webpack",
    "build:webpack:dev": "webpack -d",
    "build:prod": "webpack -p",
    "pub": "npm run build:prod && npm version patch && npm publish && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeculiarVentures/webcrypto-liner"
  },
  "dependencies": {
    "asmcrypto.js": "^0.22.0",
    "des.js": "^1.0.0",
    "elliptic": "^6.4.1",
    "webcrypto-core": "^0.1.25"
  },
  "devDependencies": {
    "@types/node": "^10.12.12",
    "ts-loader": "^5.3.1",
    "tslib": "^1.9.3",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "keywords": [
    "crypto",
    "webcrypto",
    "rsa",
    "aes",
    "ec",
    "secp256k1",
    "sha"
  ],
  "author": "PeculiarVentures",
  "license": "MIT"
}
