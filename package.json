{
  "name": "webcrypto-liner",
  "version": "0.1.4",
  "description": "A WebCrypto pollyfill that \"smooths out\" the rough-edges in existing User Agent implementations.",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:webpack",
    "build:es5": "tsc --module commonjs --target es5",
    "build:es2015": "tsc --module es2015 --target es2015",
    "build:rollup": "npm run build:es2015 && rollup -o=index.js -i=build/src/index.js",
    "build:webpack": "webpack",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeculiarVentures/webcrypto-liner"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "*",
    "asmcrypto.js": "*",
    "elliptic": "*",
    "ts-loader": "^0.9.5",
    "webcrypto-core": "git+https://github.com/PeculiarVentures/webcrypto-core#types"
  },
  "author": "PeculiarVentures",
  "license": "MIT"
}
